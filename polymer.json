{
  "entrypoint": "index.html",
  "shell": "src/landing-app.js",
  "fragments": [
   "src/lazy-resources.html",
   "src/landing-set-password.js",
   "src/landing-reset-password.js",
   "src/landing-buy-license.html",
   "src/landing-request-pricing.html"
  ],
  "sources": [
   "src/**/*",
   "data/**/*",
   "images/**/*",
   "app.yaml"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/shadycss/entrypoints/apply-shim.js",
    "node_modules/@webcomponents/webcomponentsjs/webcomponents-loader.js",
    "node_modules/@polymer/gold-cc-cvc-input/*.png",
    "node_modules/@polymer/gold-cc-input/images/*.png",
    "tumblrBadge-1.1.js"
  ],
  "builds": [{
    "name": "bundled",
    "bundle": true,
    "js": {"minify": true},
    "css": {"minify": true},
    "html": {"minify": true}
  }],
  "lint": {
    "rules": ["polymer-3"]
  }
}
